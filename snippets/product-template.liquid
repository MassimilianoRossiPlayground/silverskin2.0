                {% if shop.taxes_included %}
                  {{ 'products.product.include_taxes' | t }}
                {% endif %}
                {% if shop.shipping_policy.body != blank %}
                  {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                {% endif %}

              </div>
            {% endif %}


            <div class="product-evidence-box product-evidence-box-mob">

              <div class="shipping-return-policy">
                <div class="box-group-del">
                  <div class="box-del">
                    <img src="" class="lazyload" data-sizes="auto" data-src="https://cdn.shopify.com/s/files/1/0285/3706/1424/files/pdp-shipping-icon_2x_e0d3c320-1fce-484b-bc07-8a88b1207ba1.png?v=1610984327" alt="silverskin">
                    <div>
                      <a href="/pages/spedizioni-e-consegna" target="_blank">
                        <span>Consegna in Italia in 24/48h lavorative</span>
                      </a>
                    </div>
                  </div>
                  <div class="box-del">
                    <img src="" class="lazyload" data-sizes="auto" data-src="https://cdn.shopify.com/s/files/1/0285/3706/1424/files/pdp-return-icon_2x_a69cb4bf-9d8b-4062-a965-36f2fc9a425e.png?v=1610984327" alt="silverskin">
                    <div>
                      <a href="/pages/resi" target="_blank">
                        <ly-as-442620>30 giorni per reso e cambio</ly-as-442620>
                      </a>
                    </div>
                  </div>
                </div>
              </div>
            </div>


            <div class="trustpilot-widget  trustpilot-widget-mob trustpilot-product-mini" data-locale="{{languageWidget}}" data-template-id="54d39695764ea907c0f34825" data-businessunit-id="5de514fbf25ac90001046fb1" data-style-height="24px" data-style-width="100%" data-theme="light" data-sku="{{ current_variant.sku }}" data-no-reviews="hide" data-scroll-to-list="true" data-style-alignment="center">
              <a href="https://it.trustpilot.com/review/www.silverskin.it" target="_blank" rel="noopener">Trustpilot</a>
