              {%- assign languageWidget = "it-IT" -%}
            {% endif %}
            {% if localization.language.iso_code == 'de' %}
              {%- assign languageWidget = "de-DE" -%}
            {% endif %}
            {% if localization.language.iso_code == 'en' %}
              {%- assign languageWidget = "en-GB" -%}
            {% endif %}
